{
  "name": "ai-kid-print-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Kid Print",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "convert:png-to-svg": "tsx scripts/convert-png-to-svg.ts",
    "compress:png": "tsx scripts/compress-png.ts",
    "compress:bigpng": "tsx scripts/compress-png.ts ./public/uploads/bigpng",
    "compress:bigpng:aggressive": "tsx scripts/compress-png.ts ./public/uploads/bigpng aggressive",
    "report:letters-crop": "tsx scripts/crop-report.ts",
    "cleanup:storage": "node dist/scripts/cleanupStorage.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@image-tracer-ts/nodejs": "^1.0.4",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "potrace": "^2.1.8",
    "puppeteer": "^24.31.0",
    "sharp": "^0.34.5",
    "svg-png-converter": "^0.0.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "fast-check": "^4.5.3",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
